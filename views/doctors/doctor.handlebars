
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item"><a href="/doctors">Doctors</a></li>
            <li class="breadcrumb-item active" aria-current="page">{{doctor.name}}</li>
        </ol>
    </nav>

    <div class="row">
        <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12">
        {{!-- <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12"> --}}

            <div class="row">
                <div class="col-xl-4 col-md-12">
                    <a class="avatar" href="/"><div class="crop"><img src="/img/_demo/doctor1.jpg" alt="" /></div></a>
                    {{!-- <div class="text-success" data-toggle="tooltip" data-placement="top" title="Participant in the program of insurance points"><i class="fas fa-heartbeat"></i> Insurance</div> --}}
                </div>
                <!--/.col-->
                <div class="col-xl-8 col-md-12">
                    <div><a href="/doctors">{{doctor.specialization}}</a></div>
                    <h4><a  href="/clinics/{{doctor.hospital.id}}">{{doctor.hospital.name}}</h4>
                    <p><a class="map-toggle" data-name="{{doctor.hospital.name}}" href="/doctors">{{doctor.hospital.location.address}}</a></p>
                    <div>Work experience <strong>9+</strong> years</div>
                    <div>First visit from = <strong>100 MPT</strong> <br />+ <strong class="text-success">100 MPT bonus</strong> for next visits</div>
                </div>
                <!--/.col-->
            </div>
            <!--/.row-->

            <br />

            <div class="row">
                <div class="col">
                    <h3>Professional statement</h3>
                    <p>{{doctor.statement}}</p>

                    <hr />

                    <h3>All services</h3>
                    <ul class="list-inline">
                        {{#each doctor.specializations as |specialization|}}
                        <li class="list-inline-item">{{specialization}}</li>
                        {{/each}}
                    </ul>

                    <hr />

                    <h3>Education</h3>

                    <ul>
                        {{#each doctor.education as |ed|}}
                        <li><i class="fas fa-graduation-cap"></i> <a class="" href="#">{{ed.university}}</a> - {{ed.degree}} <br /><em>Graduate of {{ed.graduated}}</em></li>
                        {{/each}}
                    </ul>

                </div>
                <!--/.col-->
            </div>
            <!-- end row -->

        </div>
        <!--/.col-->
        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">

            <nav class="sticky-top">

                <div class="row">
				
                    <div class="col-xl-12 col-lg-12">
                        <div class="wave text-center">
                            <h4>Rating <strong class="text-success">{{doctor.rate}}</strong></h4>
                            <p>based on <strong><a class="" href="#opinions">{{doctor.ratings.length}} opinions</a></strong></p> 

                            <ul class="rating"> 
							
                                <li class="row">
                                    <div class="col text-right">
                                        Knowledge
                                    </div>
                                    <div class="col">
                                        <div class="progress progress-striped">
                                            <div class="progress-bar bg-warning" role="progressbar" aria-valuenow="{{percentage doctor.opinion.knowledge 10}}" aria-valuemin="0" aria-valuemax="100" style="width: {{percentage doctor.opinion.knowledge 10}}%">
                                                <span class="sr-only">{{doctor.opinion.knowledge}}/10</span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/.col-->
                                </li>
                                <!-- end row -->
                                <li class="row">
                                    <div class="col text-right">
                                        Skills
                                    </div>
                                    <div class="col">
                                        <div class="progress progress-striped">
                                            <div class="progress-bar bg-warning" role="progressbar" aria-valuenow="{{percentage doctor.opinion.skills 10}}" aria-valuemin="0" aria-valuemax="100" style="width: {{percentage doctor.opinion.skills 10}}%">
                                                <span class="sr-only">{{doctor.opinion.skills}}/10</span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/.col-->
                                </li>
                                <!-- end row -->
                                <li class="row">
                                    <div class="col text-right">
                                        Attention
                                    </div>
                                    <div class="col">
                                        <div class="progress progress-striped">
                                            <div class="progress-bar bg-warning" role="progressbar" aria-valuenow="{{percentage doctor.opinion.attention 10}}" aria-valuemin="0" aria-valuemax="100" style="width: {{percentage doctor.opinion.attention 10}}%">
                                                <span class="sr-only">{{doctor.opinion.attention}}/10</span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/.col-->
                                </li>
                                <!-- end row -->
                                <li class="row">
                                    <div class="col text-right">
                                        Price / Quality
                                    </div>
                                    <div class="col">
                                        <div class="progress progress-striped">
                                            <div class="progress-bar bg-warning" role="progressbar" aria-valuenow="{{percentage doctor.opinion.priceQuality 10}}" aria-valuemin="0" aria-valuemax="100" style="width: {{percentage doctor.opinion.priceQuality 10}}%">
                                                <span class="sr-only">{{doctor.opinion.priceQuality}}/10</span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/.col-->
                                </li>
                                <!-- end row -->

                            </ul>
                            <!--/.rating-->
							<a class="btn btn-primary btn-lg justify" href="/booking?doctorId={{doctor.id}}">Book to visit</a>
                            <p hidden>In favorites at <strong>462</strong> users</p>

                            <a hidden class="btn btn-secondary justify" href="#"><i class="far fa-heart"></i> Add to favorite</a>

                        </div>
                        <!--/.wave-->
                    </div>
                    <!--/.col-->
                </div>
                <!--/.row-->
 
            </nav>
 
        </div>

